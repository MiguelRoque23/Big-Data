    "import sys\n",
    "\n",
    "\n",
    "TAB_CHAR = '\\t'\n",
    "\n",
    "for line in sys.stdin:\n",
    "    for token in line.strip().split(\" \"):\n",
    "        if token: \n",
    "            print(token + TAB_CHAR + '1')"
